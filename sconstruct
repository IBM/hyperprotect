env = Environment()
env.Install("$prefix/usr/bin", "kp")
env.Install("$prefix/usr/lib/systemd/system", "keyprotect.service")
ib = env.Alias("install-bin", "$prefix/usr/bin")
il = env.Alias("install-lib", "$prefox/usr/lib/systemd/system")
env.Alias("install", [ib, il])
